{
  "name": "<%= projectName %>",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.x",
    "<%=packageManager%>": ">= <%= packageManagerVersion %>"
  },
  "description": "•\tVisual Studio Code: https://code.visualstudio.com/download<br> •\tGit: https://git-scm.com/download/win <br> •\tGit Extensions: https://github.com/gitextensions/gitextensions/releases <br> •\tNode.js: https://nodejs.org/en/download/",
  "main": "config-overrides.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:ci": "cross-env CI=true npm test -- --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=lcov --coverageReporters=html",
    "eject": "react-scripts eject",
    "eslint:report": "eslint --fix --ext .js .",
    "prettier": "prettier --write **/*.js",
    "prepack": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@axa-fr/react-oidc-context": "3.1.6",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "5.11.9",
    "@mui/material": "^5.11.10",
    "@testing-library/react": "^14.0.0",
    "@totalsoft_oss/rocket-ui.core": "^2.0.9",
    "@totalsoft_oss/rocket-ui.themes": "^2.0.5",
    "@totalsoft/change-tracking-react": "^5.0.22",
    "@totalsoft/pure-validations-react": "^5.0.22",
    "@totalsoft/rules-algebra-react": "^5.0.22",
    "@totalsoft/validations": "^1.0.5",
    "apollo-link-mock": "^1.0.1",
    "apollo-upload-client": "^17.0.0",
    "file-saver": "^2.0.5",
    "graphql": "^16.6.0",
<%_ if (withSubscription) { _%>
    "graphql-ws": "^5.11.3",
<%_ } _%>
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "lodash.curry": "^4.1.1",
    "moment": "2.29.4",
    "oidc-client": "^1.11.5",
    "omit-deep-lodash": "^1.1.7",
    "ramda": "0.28.0",
    "react": "^18.2.0",
    "react-country-flag": "3.0.2",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "react-super-responsive-table": "^5.2.1",
    "simplebar-react": "^3.2.1",
    "tss-react": "^4.6.1",
    "url-search-params-polyfill": "^8.1.1",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-pipeline-operator": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest-junit": "^15.0.0",
    "mini-css-extract-plugin": "2.7.2",
    "prettier": "^2.8.4",
    "react-app-rewired": "^2.2.1",
    "react-error-overlay": "6.0.11"
  },
  "lint-staged": {
    "**/*.+(js|md|css|graphql|json)": "prettier --write"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ]
    ]
  }
}
