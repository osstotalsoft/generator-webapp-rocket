    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest-junit": "^13.0.0",
    "prettier": "^2.5.1",
    "react-app-rewired": "^2.1.8",
    "react-error-overlay": "6.0.9"
